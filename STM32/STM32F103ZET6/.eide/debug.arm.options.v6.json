{
    "version": 3,
    "beforeBuildTasks": [],
    "afterBuildTasks": [],
    "global": {
        "output-debug-info": "enable"
    },
    "c/cpp-compiler": {
        "optimization": "level-0",
        "language-c": "c11",
        "language-cpp": "c++14",
        "link-time-optimization": true,
        "warnings": "ac5-like-warnings",
        "one-elf-section-per-function": true,
        "misc-controls": "-Wno-error=armcc-pragma-import -Wno-error=armcc-pragma-arm"
    },
    "asm-compiler": {
        "$use": "asm-auto"
    },
    "linker": {
        "output-format": "elf",
        "misc-controls": "--diag_suppress=L6329"
    }
}